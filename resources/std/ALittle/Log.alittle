
register namespace ALittle;

// 设置自定义的日志函数，如果没有设置，那么默认使用lua的print
// func回调的第一个参数是打印的内容，第二个参数是打印的等级参考LogLevel枚举
public enum LogLevel
{
    INFO = 0,
    WARN = 1,
    ERROR = 2,
}
public static SetLogCallback(Functor<(string, int)> func) {}
public static Log(any arg1, any arg2, any arg3, any arg4, any arg5, any arg6) { }
public static Warn(any arg1, any arg2, any arg3) { }
public static Error(any arg1, any arg2, any arg3) { }